   ],
   "source": [
    "tab1 = (veh.groupby('is_moto')['fatal']\n",
    "          .agg(['sum','count'])\n",
    "          .rename(columns={'sum':'fatal_sum','count':'n'}))\n",
    "\n",
    "if {True, False}.issubset(set(tab1.index)):\n",
    "    count = [int(tab1.loc[True, 'fatal_sum']), int(tab1.loc[False, 'fatal_sum'])]\n",
    "    nobs  = [int(tab1.loc[True, 'n']),         int(tab1.loc[False, 'n'])]\n",
    "    z1, p1 = proportions_ztest(count, nobs, alternative='larger')\n",
    "    print('Н0: Мотоциклы чаще других ТС попадают в ДТП со смертельным исходом')\n",
    "    print(f\"p_moto = {count[0]}/{nobs[0]} = {count[0]/nobs[0]:.4f}\")\n",
    "    print(f\"p_other= {count[1]}/{nobs[1]} = {count[1]/nobs[1]:.4f}\")\n",
    "    print(f\"Z = {z1:.3f}, p-value = {p1:.4g}\")\n",
    "    print(\"Вывод:\", \"Отвергаем нулевую гипотезу\" if p1 < 0.05 else \"Мотоциклы попадают в ДТП со смертельным исходом чаще других ТС\")\n",
    "else:\n",
    "    print(\"В данных нет одной из групп (мото или не мото)\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "id": "4113a254-1d57-4e09-8fce-40c3c3167903",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(223445, 7)\n"
     ]
    }
   ],
   "source": [
    "point = data.get(\"point\", [])\n",
    "def to_list_of_dicts(v):\n",
    "    # уже список словарей\n",
    "    if isinstance(v, list):\n",
    "        return [i for i in v if isinstance(i, dict)]\n",
    "    # один словарь\n",
    "    if isinstance(v, dict):\n",
    "        return [v]\n",
    "    if pd.isna(v):\n",
    "        return []\n",
    "    # пробуем распарсить\n",
    "    if isinstance(v, str):\n",
    "        s = v.strip()\n",
    "        if not s or s.lower() in ('null', 'none', 'nan'):\n",
    "            return []\n",
    "        for parser in (ast.literal_eval, json.loads):\n",
    "            try:\n",
    "                x = parser(s)\n",
    "                if isinstance(x, dict):\n",
    "                    return [x]\n",
    "                if isinstance(x, list):\n",
    "                    return [i for i in x if isinstance(i, dict)]\n",
    "            except Exception:\n",
    "                pass\n",
    "    return []\n",
    "\n",
    "data['vehicles'] = data['vehicles'].map(to_list_of_dicts)\n",
    "vehicles = data[['id', 'vehicles']].explode('vehicles', ignore_index=True)\n",
    "vehicles = vehicles[vehicles['vehicles'].apply(lambda x: isinstance(x, dict))].reset_index(drop=True)\n",
    "\n",
    "vehicles_df = vehicles[['id']].join(pd.json_normalize(vehicles['vehicles']))\n",
    "\n",
    "print(vehicles_df.shape)"
   ]
  },
