    "acc = (full_long\n",
    "       .drop_duplicates('id')\n",
    "       [['id', 'road_conditions', 'severity']]\n",
    "       .dropna(subset=['road_conditions', 'severity']))\n",
    "\n",
    "# порядок уровней тяжести\n",
    "sev_order_full = ['Легкий', 'Тяжёлый', 'С погибшими']\n",
    "sev_order = [s for s in sev_order_full if s in acc['severity'].unique()]\n",
    "\n",
    "# доли по состоянию дороги\n",
    "ct = pd.crosstab(\n",
    "    acc['road_conditions'],\n",
    "    pd.Categorical(acc['severity'], categories=sev_order, ordered=True),\n",
    "    normalize ='index'\n",
    ")\n",
    "plt.figure(figsize=(8,6))\n",
    "sns.heatmap(ct * 100, annot=True, fmt='.1f', cmap='Reds')\n",
    "plt.xlabel('Тяжесть ДТП')\n",
    "plt.ylabel('Состояние дороги')\n",
    "plt.title('Тяжесть ДТП по состоянию дорог, %')\n",
    "plt.tight_layout()\n",
    "plt.show()"
