   "source": [
    "def to_list_of_dicts(v):\n",
    "    # уже список словарей\n",
    "    if isinstance(v, list):\n",
    "        return [i for i in v if isinstance(i, dict)]\n",
    "    # один словарь\n",
    "    if isinstance(v, dict):\n",
    "        return [v]\n",
    "    if pd.isna(v):\n",
    "        return []\n",
    "    # пробуем распарсить\n",
    "    if isinstance(v, str):\n",
    "        s = v.strip()\n",
    "        if not s or s.lower() in ('null', 'none', 'nan'):\n",
    "            return []\n",
    "        for parser in (ast.literal_eval, json.loads):\n",
    "            try:\n",
    "                x = parser(s)\n",
    "                if isinstance(x, dict):\n",
    "                    return [x]\n",
    "                if isinstance(x, list):\n",
    "                    return [i for i in x if isinstance(i, dict)]\n",
    "            except Exception:\n",
    "                pass\n",
    "    return []\n",
    "\n",
    "data['point'] = data['point'].map(to_list_of_dicts)\n",
    "point = data[['id', 'point']].explode('point', ignore_index=True)\n",
    "point = point[point['point'].apply(lambda x: isinstance(x, dict))].reset_index(drop=True)\n",
    "\n",
    "point_df = point[['id']].join(pd.json_normalize(point['point']))\n",
    "\n",
    "print(point_df.shape)\n",
    "print(point_df.columns.tolist())"
   ]
  },
