    "hs = participants_df['health_status'].astype(str).str.strip()\n",
    "dead = hs.str.contains(r'скончал', case=False, na=False)\n",
    "\n",
    "injured = (\n",
    "    hs.str.contains(r'ранен', case=False, na=False)\n",
    "    | (hs.str.contains(r'получ', case=False, na=False) \n",
    "       & hs.str.contains(r'(травм|телесн|поврежд)', case=False, na=False))\n",
    ")\n",
    "\n",
    "ok = hs.str.contains(r'не\\s*пострадал', case=False, na=False)\n",
    "\n",
    "participants_df['health_status'] = np.select(\n",
    "    [dead, injured, ok],\n",
    "    ['Погиб', 'Ранен', 'Не пострадал'],\n",
    "    default=participants_df['health_status']\n",
    ")\n",
    "participants_df.tail()"
