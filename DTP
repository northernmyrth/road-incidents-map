   "source": [
    "def to_list_of_dicts(v):\n",
    "    # уже список словарей\n",
    "    if isinstance(v, list):\n",
    "        return [i for i in v if isinstance(i, dict)]\n",
    "    # один словарь\n",
    "    if isinstance(v, dict):\n",
    "        return [v]\n",
    "    if pd.isna(v):\n",
    "        return []\n",
    "    # пробуем распарсить\n",
    "    if isinstance(v, str):\n",
    "        s = v.strip()\n",
    "        if not s or s.lower() in ('null', 'none', 'nan'):\n",
    "            return []\n",
    "        for parser in (ast.literal_eval, json.loads):\n",
    "            try:\n",
    "                x = parser(s)\n",
    "                if isinstance(x, dict):\n",
    "                    return [x]\n",
    "                if isinstance(x, list):\n",
    "                    return [i for i in x if isinstance(i, dict)]\n",
    "            except Exception:\n",
    "                pass\n",
    "    return []\n",
    "\n",
    "data['vehicles'] = data['vehicles'].map(to_list_of_dicts)\n",
    "vehicles = data[['id', 'vehicles']].explode('vehicles', ignore_index=True)\n",
    "vehicles = vehicles[vehicles['vehicles'].apply(lambda x: isinstance(x, dict))].reset_index(drop=True)\n",
    "\n",
    "vehicles_df = vehicles[['id']].join(pd.json_normalize(vehicles['vehicles']))\n",
    "\n",
    "print(vehicles_df.shape)\n",
    "print(vehicles_df.columns.tolist())"
   ]
