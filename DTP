     ]
    }
   ],
   "source": [
    "records = [] #создаем пустой список для загрузки\n",
    "directory = 'DTP'\n",
    "# перебираем все файлы geojson в папке\n",
    "for filename in os.listdir(directory):\n",
    "    if filename.endswith(\".geojson\"):\n",
    "        path = os.path.join(directory, filename)\n",
    "        try:\n",
    "            with open(path, encoding=\"utf-8\") as f:\n",
    "                data = json.load(f)\n",
    "            accidents = data[\"features\"]\n",
    "\n",
    "            record = []\n",
    "            for feature in accidents:\n",
    "                props = feature[\"properties\"].copy()\n",
    "                props[\"geometry\"] = feature[\"geometry\"]\n",
    "                record.append(props)\n",
    "\n",
    "            records.extend(record)\n",
    "            print(f\"Загружено {len(record)} строк из {filename}\")\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f\"Ошибка при обработке {filename}: {e}\")\n",
    "\n",
    "# собираем в один датафрейм\n",
    "data = pd.DataFrame(records)\n",
    "\n",
    "print(f\"Итого строк во всех файлах: {len(data)}\")\n",
    "\n",
    "# Удаляем лишнюю переменную\n",
    "del records"
   ]
  },
