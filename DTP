   "source": [
    "rules = [\n",
    "    (r'(переход.*вне.*пешеходн|неустановленном месте)', 'Переход через проезжую часть вне пешеходного перехода'),\n",
    "    (r'неподчинени[ея].*сигнал', 'Неподчинение сигналам регулирования'),\n",
    "    (r'световозвращающ', 'Отсутствие световозвращающих элементов'),\n",
    "    (r'неожиданн.*выход', 'Иные нарушения'),\n",
    "    (r'движение вдоль.*обочин\\w*', 'Движение вдоль проезжей части'),\n",
    "    (r'ходьб[аы] вдоль.*тротуар\\w*', 'Движение вдоль проезжей части'),\n",
    "    (r'игра на проезжей части', 'Движение вдоль проезжей части'),\n",
    "    (r'в состоянии алкогольного опьян', 'Пешеход в состоянии опьянения'),\n",
    "    (r'в состоянии наркотическ', 'Пешеход в состоянии опьянения'),\n",
    "    (r'до 7 лет', 'Нахождение на проезжей части'),\n",
    "    (r'нахождени[ея] на проезжей части.*без цели .*пер', 'Нахождение на проезжей части'),\n",
    "    (r'иные наруш', 'Иные нарушения'),\n",
    "]\n",
    "\n",
    "def pick_main_violation(val):\n",
    "    # приводим к одной строке\n",
    "    if isinstance(val, list):\n",
    "        text = ' | '.join(map(str, val))\n",
    "        is_empty_list = len(val) == 0\n",
    "    else:\n",
    "        text = '' if pd.isna(val) else str(val)\n",
    "        is_empty_list = text.strip() in ('', '[]')\n",
    "    low = text.lower()\n",
    "\n",
    "    for pat, label in rules:\n",
    "        if re.search(pat, low, flags=re.I):\n",
    "            return label\n",
    "\n",
    "    if is_empty_list:\n",
    "        return 'Нарушения не зафиксированы'\n",
    "    return 'Иные нарушения'\n",
    "\n",
    "participants_df['violation_main'] = participants_df['violations'].apply(pick_main_violation)\n",
    "print(participants_df['violation_main'].value_counts().head(10))"
   ]
  },
