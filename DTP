    "# Создание списка приоритетов для обработки значений\n",
    "priority_list_nearby = ['unregulated_crossroad', 'unregulated_crosswalk', 'regulated_crossroad', 'regulated_crosswalk', 'school', 'train_crossroad','transport_stop', 'police', 'mall', 'hospital', 'church', 'private_houses', 'residential_buildings', 'bridge', 'other_buildings']\n",
    "\n",
    "def process_nearby(nearby_input):\n",
    "    if pd.isna(nearby_input):\n",
    "        return nearby_input\n",
    "    \n",
    "    nearby_parts = [elem.strip() for elem in re.split(r',\\s*', nearby_input)]\n",
    "    \n",
    "    nearby_mapped = []\n",
    "    for nearby_elem in nearby_parts:\n",
    "        if nearby_elem in replacement_dict_nearby:\n",
    "            nearby_mapped.append(replacement_dict_nearby[nearby_elem])\n",
    "    \n",
    "    if nearby_mapped:\n",
    "        for priority_item in priority_list_nearby:\n",
    "            if priority_item in nearby_mapped:\n",
    "                return priority_item\n",
    "        return nearby_mapped[0]  # Возвращаем первую, если ни одна не в приоритете\n",
    "    else:\n",
    "        return 'other_buildings'  # Категория по умолчанию для необработанных\n",
    "\n",
    "if 'nearby' in data.columns:\n",
    "    data['nearby_processed'] = data['nearby'].apply(process_nearby)\n",
    "    print(data['nearby_processed'].value_counts())\n",
    "else:\n",
    "    print(\"Столбец 'nearby' не найден.\")"
   ]
